<div class="elv-toc" data-pagefind-ignore>
<details-force-state>
	<details open>
		<summary class="btn-primary benchnine rainbow-active rainbow-active-noanim">Menu</summary>

		{%- if eleventyNavigation -%}
			{%- set navKey = eleventyNavigation.key -%}
		{%- endif -%}
		{{ collections.sidebarNav | eleventyNavigation | eleventyNavigationToHtml({ isChildList: true, activeKey: navKey, listClass: 'elv-toc-list', activeListItemClass: 'elv-toc-active' }) | safe }}

	</details>
</details-force-state>
</div>
 